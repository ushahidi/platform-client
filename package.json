    "watch": "gulp",
    "start": "gulp serve:static",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js",
    "lint": "eslint app --ext .jsx,.js",
    "lint-fix": "eslint . --fix",
    "precommit": "lint-staged",
    "cz": "git-cz",
    "retry": "git-cz --retry",
    "test": "karma start ./test/karma.conf.js --single-run && jest",
    "tdd:karma": "karma start ./test/karma.conf.js --auto-watch",
    "tdd:jest": "jest --watch"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.jsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest-setup.js",
    "testPathIgnorePatterns": [
      "./node_modules",
      "./test"
    ],
    "modulePaths": [
      "app"
    ],
    "setupFiles": [
      "./app/react/test/browserMocks.js"
    ]
  },
